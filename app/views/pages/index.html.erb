<h1>Pages#index</h1>





<% if user_signed_in? %>
  <div>Hello <%= current_user.email %> pick your tournament</div>
  <div id="sign_in"><%= link_to "Sign out", destroy_user_session_path, :method => :delete %></div>
<% else %>
<h1>welcome</h1>




<div id="sign_in">
  <%= link_to "New", new_user_registration_path, :method => :get %>
  <%= link_to "Sign in", new_user_session_path, :method => :get %>
  <%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
</div>


<% end %>

<%= render template:"tournaments/index" %>


